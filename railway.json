{
  "build": {
    "commands": [
      "cd server_bot",
      "pip install -r requirements.txt"
    ]
  },
  "start": {
    "command": "cd server_bot && python main.py"
  },
  "volumes": [
    {
      "mountPath": "/app/server_bot/server_management.db",
      "name": "server_bot_database"
    }
  ]
}